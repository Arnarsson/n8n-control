# Point directly at the public API root (include the /api/v1 suffix).
N8N_API_URL=https://n8n.example.com/api/v1

# Generate in n8n: Settings → n8n API → Create API key
# Will be sent as header: X-N8N-API-KEY
N8N_API_KEY=replace_me

# Optional: try new API execution endpoint if your n8n supports it (late 2025+)
N8N_EXECUTE_ENDPOINT=false

# Optional: secret header added on webhook calls for extra protection
N8N_WEBHOOK_SECRET=
